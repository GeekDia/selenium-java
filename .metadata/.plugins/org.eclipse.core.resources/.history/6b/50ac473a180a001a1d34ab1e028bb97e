package day1;

public class DataVariables {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a =17, b=15, c, age, day=2;
		System.out.println("Before swap : " + "a = " +a + " and b = " + b);
		c = a ;
		a = b ; 
		b = c ; 
		int array[] = new int[10];
		
		System.out.println("after swap : " + "a = " +a + " and b = " + b);
		
		String name = "Baba";
		age = 35;
		if(name=="Bamba" &&(age >=33) ){
			System.out.println("Your name is Bamba");
			
		}else if(name!="Bamba" && (age>=33)){
			System.out.println("You were not Bamba but you were eligible to vote");
			
		} else {
			System.out.println("You were not Bamba");

		}
		
		if(a%2 == 0){
			System.out.println("a is an even number");
			
		}else System.out.println("a is an ODD number");
		
		switch(day){
		case 7:
			System.out.println("Today is sunday");
		break;
		case 6:
			System.out.println("Today is saturday");
		break;
		case 5:
			System.out.println("Today is friday");
		break;
		case 4:
			System.out.println("Today is Thursday");
		break;
		case 3:
			System.out.println("Today is Wednesday");
		break;
		case 2:
			System.out.println("Today is Tuesday");
		break;
		case 1:
			System.out.println("Today is Monday");
		break;
		default:
			System.out.println("We were in word");
		break;
		}
		
		int i = 1;
		while(i <=10){
			
			
			System.out.println(" i vaut: " + i);
			array[i] = i;
			i++;
		}
		
		for(;b > 0; b--){
			//System.out.println("b is still superior to 0  and has value : " + b);
			if(b==10){
				break;
			}
			System.out.println("b is still superior to 0  and has value : " + b);
		}
		for(int y = 1; y<=10; y++){
			if(y==3 || y==5 || y==7){
				continue;
			}
			System.out.println("y has value : " + y);
		}
		
		int z, t, v;
		z = -10 ; t=-5; v=-8;
		
		if (z>t && z>v){
			System.out.println("z is greater than t and v");
		}else if (t>z && t>v){
			System.out.println("t is greater than z and v");
		}
		else if (v>z && t<v){
			System.out.println("v is greater than z and t");
		}
	}

}
