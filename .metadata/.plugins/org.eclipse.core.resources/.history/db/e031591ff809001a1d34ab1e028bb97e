package day1;

public class DataVariables {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a =10, b=20, c;
		System.out.println("Before swap : " + "a = " +a + " and b = " + b);
		c = a ;
		a = b ; 
		b = c ; 
		
		System.out.println("after swap : " + "a = " +a + " and b = " + b);
	}

}
